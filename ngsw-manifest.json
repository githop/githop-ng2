{
  "external": {
    "urls": [
      { "url": "https://fonts.googleapis.com/css?family=Fira+Mono" },
      { "url": "https://fonts.gstatic.com/s/firamono/v6/SlRWfq1zeqXiYWAN-lnG-pBw1xU1rKptJj_0jans920.woff2" }

    ]
  },
  "routing": {
    "index": "/index.html",
    "routes": {
      "/": {
        "prefix": false
      },
      "/resume": {
        "prefix": false
      },
      "/blog": {
        "prefix": true
      }
    }
  },
  "dynamic": {
    "group": [
      {
        "name": "articles",
        "urls": {
          "/api/articles": {
            "match": "prefix"
          }
        },
        "cache": {
          "optimizeFor": "performance",
          "strategy": "lru",
          "maxAgeMs": 3600000,
          "maxEntries": 20
        }
      }
    ]
  }
}
